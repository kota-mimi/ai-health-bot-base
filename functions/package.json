{
  "name": "ai-diet-functions",
  "version": "1.0.0",
  "description": "Firebase Functions for AI Diet App",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.0.1",
    "@google/generative-ai": "^0.24.1",
    "@line/bot-sdk": "^9.3.0",
    "stripe": "^19.3.1",
    "dotenv": "^17.2.3",
    "date-fns": "^4.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "firebase-functions-test": "^3.3.0"
  },
  "private": true
}